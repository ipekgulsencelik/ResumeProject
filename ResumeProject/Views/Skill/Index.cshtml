@model List<ResumeProject.Models.TblSkill>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    int count = 0;
}

<h2>Skill List</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Skill Title</th>
        <th>Delete</th>
        <th>Update</th>
    </tr>
    @foreach (var item in Model)
    {
        count++;
        <tr>
            <td>@count</td>
            <td>@item.SkillTitle</td>
            <td><a href="/Skill/DeleteSkill/@item.SkillID" class="btn btn-outline-danger">Delete</a></td>
            <td><a href="/Skill/UpdateSkill/@item.SkillID" class="btn btn-outline-success">Update</a></td>
        </tr>
    }
</table>
<a href="/Skill/AddSkill/" class="btn btn-outline-primary">Create New Skill</a>